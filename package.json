{
  "name": "vilatur",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "format:repo": "npm run format && npm run lint:repo -- --fix",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:repo": "npm run lint -- --config .eslintrc.repo.js",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc && tsc -p cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "seed:build": "tsc --project ./tsconfig.seed.json && tsc-alias -p ./tsconfig.seed.json"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@chakra-ui/react": "^2.8.0",
    "@conform-to/react": "^0.7.2",
    "@conform-to/zod": "^0.7.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@playwright/test": "^1.35.1",
    "@prisma/client": "^4.15.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-hover-card": "^1.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-toast": "^1.1.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@remix-run/css-bundle": "^1.18.0",
    "@remix-run/express": "^1.18.0",
    "@remix-run/node": "^1.18.0",
    "@remix-run/react": "^1.18.0",
    "@remix-run/serve": "^1.18.0",
    "@remix-run/server-runtime": "^1.18.0",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/morgan": "^1.9.4",
    "bcryptjs": "^2.4.3",
    "cachified": "^3.5.4",
    "class-variance-authority": "^0.6.1",
    "close-with-grace": "^1.2.0",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "enforce-unique": "^1.1.1",
    "eslint-plugin-remix-react-routes": "^1.0.5",
    "execa": "^7.1.1",
    "express": "^4.18.2",
    "framer-motion": "^10.15.0",
    "get-port": "^7.0.0",
    "helmet": "^7.0.0",
    "isbot": "^3.6.12",
    "lru-cache": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-slick": "^0.29.0",
    "remix-auth": "^3.4.0",
    "remix-auth-form": "^1.3.0",
    "slick-carousel": "^1.8.1",
    "tailwind-merge": "^1.13.2",
    "tailwindcss-radix": "^2.8.0",
    "thirty-two": "^1.0.2",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@flydotio/dockerfile": "^0.2.16",
    "@remix-run/dev": "^1.18.0",
    "@remix-run/eslint-config": "^1.18.0",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/better-sqlite3": "^7.6.4",
    "@types/eslint": "^8.40.1",
    "@types/node": "^18.16.18",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/react-slick": "^0.23.10",
